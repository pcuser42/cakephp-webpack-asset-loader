{
    "name": "pcuser42/cakephp-webpack-asset-loader",
    "description": "Webpack Asset handling plugin for CakePHP. It takes an exported manifest.json file and then automatically loads all assets for an specified entry point.",
    "type": "cakephp-plugin",
    "license": "MIT",
    "homepage": "https://github.com/pcuser42/cakephp-webpack-asset-loader",
    "authors": [
        {
            "name": "Daniel Melchior @ Interweber",
            "homepage": "https://github.com/interweberde",
            "role": "Author"
        },
        {
            "name": "Alex Burgess",
            "homepage": "https://github.com/pcuser42",
            "role": "Author"
        }
    ],
    "support": {
        "issues": "https://github.com/pcuser42/cakephp-webpack-asset-loader/issues",
        "source": "https://github.com/pcuser42/cakephp-webpack-asset-loader"
    },
    "require-dev": {
        "cakephp/cakephp": "^4.0",
        "phpstan/phpstan": "^1.9",
        "squizlabs/php_codesniffer": "^3.7",
        "interweberde/interweber-cs": "^1.0",
        "dealerdirect/phpcodesniffer-composer-installer": "^1.0",
        "sirbrillig/phpcs-variable-analysis": "^2.11",
        "ext-dom": "*"
    },
    "autoload": {
        "psr-4": {
            "Pcuser42\\WebpackAssetLoader\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pcuser42\\WebpackAssetLoader\\Test\\": "tests/"
        }
    },
    "scripts": {
        "lint": "phpstan analyse --level 7 src/ tests/",
        "cs-check": "phpcs src/ tests/",
        "cs-fix": "phpcbf src/ tests/"
    },
    "require": {
        "php": ">=7.4.0",
        "ext-json": "*",
        "ext-dom": "*",
        "phpunit/phpunit": "^9.3 || ^10.0"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
